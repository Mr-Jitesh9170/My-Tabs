{
  "manifest_version": 3,
  "name": "My-Tabs",
  "description": "A simple Chrome extension that store your favourite extensions",
  "version": "1.0",
  "action": {
    "default_popup": "mytabs.html"
  },
  "permissions": ["scripting", "activeTab","tabs"], 
  "background": {
    "service_worker": "background.js"
  }
}
