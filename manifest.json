{
  "manifest_version": 3,
  "name": "My-Tabs",
  "description": "A simple Chrome extension that store your favourite extensions",
  "version": "1.0",
  "action": {
    "default_popup": "mytabs.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  }
}
